{
	"name": "clinecn",
	"displayName": "ClineCN",
	"description": "中文版Cline，ClineCN 是一个AI编程助手，当您确认后，它可以执行创建/编辑文件、运行命令、使用浏览器等功能。ClineCN 基于 Cline 开发，是针对中文优化后的 Cline，在学习 Cline 的同时，也会加入我们创新的功能。",
	"version": "3.3.3",
	"icon": "assets/icons/icon.png",
	"galleryBanner": {
		"color": "#617A91",
		"theme": "dark"
	},
	"engines": {
		"vscode": "^1.84.0"
	},
	"author": {
		"name": "yangbo"
	},
	"license": "Apache-2.0",
	"publisher": "yangbo",
	"repository": {
		"type": "git",
		"url": "https://github.com/yangbo/clinecn"
	},
	"homepage": "https://github.com/yangbo/clinecn",
	"categories": [
		"AI",
		"Chat",
		"Programming Languages",
		"Education",
		"Snippets",
		"Testing"
	],
	"keywords": [
		"cline",
		"clinecn",
		"deepseek",
		"claude",
		"dev",
		"mcp",
		"openrouter",
		"coding",
		"agent",
		"autonomous",
		"chatgpt",
		"sonnet",
		"ai",
		"llama",
		"AI编程",
		"自动编程",
		"助手",
		"大语言模型代理人",
		"代理人"
	],
	"activationEvents": [],
	"main": "./dist/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "clinecn-ActivityBar",
					"title": "ClineCN",
					"icon": "assets/icons/icon.svg"
				}
			]
		},
		"views": {
			"clinecn-ActivityBar": [
				{
					"type": "webview",
					"id": "clinecn.SidebarProvider",
					"name": "ClineCN"
				}
			]
		},
		"commands": [
			{
				"command": "clinecn.plusButtonClicked",
				"title": "新任务",
				"icon": "$(add)"
			},
			{
				"command": "clinecn.mcpButtonClicked",
				"title": "模型上下文协议(MCP)服务器",
				"icon": "$(server)"
			},
			{
				"command": "clinecn.historyButtonClicked",
				"title": "历史",
				"icon": "$(history)"
			},
			{
				"command": "clinecn.popoutButtonClicked",
				"title": "在编辑器中打开",
				"icon": "$(link-external)"
			},
			{
				"command": "clinecn.settingsButtonClicked",
				"title": "设置",
				"icon": "$(settings-gear)"
			},
			{
				"command": "clinecn.openInNewTab",
				"title": "在新标签中打开",
				"category": "ClineCN"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "clinecn.plusButtonClicked",
					"group": "navigation@1",
					"when": "view == clinecn.SidebarProvider"
				},
				{
					"command": "clinecn.mcpButtonClicked",
					"group": "navigation@2",
					"when": "view == clinecn.SidebarProvider"
				},
				{
					"command": "clinecn.historyButtonClicked",
					"group": "navigation@3",
					"when": "view == clinecn.SidebarProvider"
				},
				{
					"command": "clinecn.popoutButtonClicked",
					"group": "navigation@4",
					"when": "view == clinecn.SidebarProvider"
				},
				{
					"command": "clinecn.settingsButtonClicked",
					"group": "navigation@5",
					"when": "view == clinecn.SidebarProvider"
				}
			]
		},
		"configuration": {
			"title": "ClineCN - 中文版Cline",
			"properties": {
				"clinecn.vsCodeLmModelSelector": {
					"type": "object",
					"properties": {
						"vendor": {
							"type": "string",
							"description": "大语言模型的供应商（例如 Alibaba）"
						},
						"family": {
							"type": "string",
							"description": "语言模型的家族（例如 DeepSeek）"
						}
					},
					"description": "VSCode大语言模型API设置"
				},
				"clinecn.mcp.mode": {
					"type": "string",
					"enum": [
						"full",
						"server-use-only",
						"off"
					],
					"enumDescriptions": [
						"启用所有 MCP 功能（服务器使用和构建指令）",
						"仅启用 MCP 服务器使用（不包括构建 MCP 服务器的指令）",
						"禁用所有 MCP 功能"
					],
					"default": "full",
					"description": "控制MCP在提示词中的包含情况，如果仅需要访问某些功能，则可减少token的使用。"
				},
				"clinecn.enableCheckpoints": {
					"type": "boolean",
					"default": true,
					"description": "允许扩展在任务过程中保存工作区检查点。"
				},
				"clinecn.disableBrowserTool": {
					"type": "boolean",
					"default": false,
					"description": "禁止扩展启动浏览器会话。"
				},
				"clinecn.modelSettings.o3Mini.reasoningEffort": {
					"type": "string",
					"enum": [
						"low",
						"medium",
						"high"
					],
					"default": "medium",
					"description": "控制使用 o3-mini 模型时的推理努力程度。更高的值可能会产生更全面但更慢的响应。"
				},
				"clinecn.chromeExecutablePath": {
					"type": "string",
					"default": null,
					"description": "Chrome 可执行文件的路径，用于浏览器功能。如果未设置，扩展将尝试自动查找或下载。"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "npm run check-types && npm run lint && node esbuild.js",
		"watch": "npm-run-all -p watch:*",
		"watch:esbuild": "node esbuild.js --watch",
		"watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
		"watch:webview": "cd webview-ui && npm run watch",
		"package": "npm run build:webview && npm run check-types && npm run lint && node esbuild.js --production",
		"compile-tests": "tsc -p ./tsconfig.test.json --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"check-types": "tsc --noEmit",
		"lint": "eslint src --ext ts",
		"format": "prettier . --check",
		"format:fix": "prettier . --write",
		"test": "vscode-test",
		"install:all": "npm install && cd webview-ui && npm install",
		"start:webview": "cd webview-ui && npm run start",
		"build:webview": "cd webview-ui && npm run build",
		"test:webview": "cd webview-ui && npm run test",
		"publish:marketplace": "vsce publish && ovsx publish",
		"publish:marketplace:prerelease": "vsce publish --pre-release && ovsx publish --pre-release",
		"prepare": "husky",
		"changeset": "changeset",
		"version-packages": "changeset version"
	},
	"devDependencies": {
		"@changesets/cli": "^2.27.12",
		"@types/chai": "^5.0.1",
		"@types/diff": "^5.2.1",
		"@types/mocha": "^10.0.7",
		"@types/node": "20.x",
		"@types/should": "^11.2.0",
		"@types/vscode": "^1.84.0",
		"@typescript-eslint/eslint-plugin": "^7.14.1",
		"@typescript-eslint/parser": "^7.11.0",
		"@vscode/test-cli": "^0.0.9",
		"@vscode/test-electron": "^2.4.0",
		"chai": "^4.3.10",
		"esbuild": "^0.25.0",
		"eslint": "^8.57.0",
		"husky": "^9.1.7",
		"nodemon": "^3.1.9",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.3.3",
		"should": "^13.2.3",
		"typescript": "^5.4.5"
	},
	"dependencies": {
		"@anthropic-ai/bedrock-sdk": "^0.10.2",
		"@anthropic-ai/sdk": "^0.26.0",
		"@anthropic-ai/vertex-sdk": "^0.4.1",
		"@google/generative-ai": "^0.18.0",
		"@mistralai/mistralai": "^1.3.6",
		"@modelcontextprotocol/sdk": "^1.0.1",
		"@types/clone-deep": "^4.0.4",
		"@types/get-folder-size": "^3.0.4",
		"@types/pdf-parse": "^1.1.4",
		"@types/turndown": "^5.0.5",
		"@vscode/codicons": "^0.0.36",
		"axios": "^1.7.4",
		"cheerio": "^1.0.0",
		"chokidar": "^4.0.1",
		"clone-deep": "^4.0.1",
		"default-shell": "^2.2.0",
		"delay": "^6.0.0",
		"diff": "^5.2.0",
		"execa": "^9.5.2",
		"fast-deep-equal": "^3.1.3",
		"firebase": "^11.2.0",
		"get-folder-size": "^5.0.0",
		"globby": "^14.0.2",
		"ignore": "^7.0.3",
		"isbinaryfile": "^5.0.2",
		"mammoth": "^1.8.0",
		"monaco-vscode-textmate-theme-converter": "^0.1.7",
		"openai": "^4.83.0",
		"os-name": "^6.0.0",
		"p-wait-for": "^5.0.2",
		"pdf-parse": "^1.1.1",
		"puppeteer-chromium-resolver": "^23.0.0",
		"puppeteer-core": "^23.4.0",
		"serialize-error": "^11.0.3",
		"simple-git": "^3.27.0",
		"strip-ansi": "^7.1.0",
		"tree-sitter-wasms": "^0.1.11",
		"turndown": "^7.2.0",
		"web-tree-sitter": "^0.22.6",
		"zod": "^3.23.8"
	}
}
